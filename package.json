{
  "name": "template-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "realtime": "node --import tsx server/realtime-server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:api": "playwright test --project=api",
    "test:e2e:browser": "playwright test --project=chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:seed": "node --import tsx tests/fixtures/seed.ts",
    "test:all": "npm run test && npm run test:e2e",
    "demo:seed": "node --import tsx tests/fixtures/demo-data/index.ts seed",
    "demo:reset": "node --import tsx tests/fixtures/demo-data/index.ts reset",
    "demo:clear": "node --import tsx tests/fixtures/demo-data/index.ts clear",
    "docs:api": "node --import tsx scripts/generate-openapi.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@hookform/resolvers": "^5.2.2",
    "@openrouter/ai-sdk-provider": "^0.0.5",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^8.38.0",
    "@tanstack/react-query": "^5.90.12",
    "@types/bcryptjs": "^2.4.6",
    "ai": "^4.0.0",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.0.7",
    "bullmq": "^5.28.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.41.0",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.454.0",
    "mammoth": "^1.7.1",
    "minio": "^8.0.2",
    "next": "^15.0.3",
    "pdf-parse": "^1.1.1",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.68.0",
    "tailwind-merge": "^2.5.4",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@bull-board/api": "^6.14.1",
    "@bull-board/ui": "^6.14.1",
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.9.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.3",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vercel": "^50.1.0",
    "vitest": "^2.1.8",
    "yaml": "^2.8.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
